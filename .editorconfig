root = true

[*]
indent_style = space
indent_size = 4
max_line_length = 150
charset = utf-8
end_of_line = lf
trim_trailing_whitespace = true
insert_final_new_line = true

[*.md]
max_line_length = 999
trim_trailing_whitespace = false

[*.bat]
end_of_line = crlf

[*.ini]
end_of_line = crlf

[*.sql]
end_of_line = crlf
