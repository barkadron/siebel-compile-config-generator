{
    "compilerOptions": {
        "strict": true,
        "target": "ES5",
        "module": "CommonJS",
        "moduleResolution": "node",
        "outDir": "build",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "importHelpers": true,
        "removeComments": true,
        "sourceMap": true
    }
}
